<div class="container"> 
  <div class="card " style="width: 18rem;">
    <img class="card-img-top" src="https://picsum.photos/id/<%= rand(1..99) %>/75/75" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"><%= @user.first_name %> <%= @user.last_name%> </h5>
      <p class="card-text"><%= @user.description %></p>
      <a><%= link_to "edit", edit_user_registration_path, class: "btn btn-primary" %></a>
    </div>
  </div>
</div>
<p>
<% @user.event_admins.each do |event| %>
<p><%= event.title %></p>
<% end %>
</p>
